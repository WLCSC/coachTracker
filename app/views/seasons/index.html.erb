<h1>Listing Seasons</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Start</th>
    </tr>
  </thead>

  <tbody>
    <% @seasons.each do |season| %>
      <tr>
        <td><%= link_to season.name, season %></td>
        <td><%= nice_date season.start %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Season', new_season_path, class: 'btn btn-primary' %>
