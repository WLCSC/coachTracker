<h1>Listing Years</h1>

<table class="table">
  <thead>
    <tr>
      <th>Years</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @years.each do |year| %>
      <tr>
        <td><%= link_to year.name, year %></td>
        <td><%= link_to 'Edit', edit_year_path(year) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Year', new_year_path, class: 'btn btn-primary' %>
