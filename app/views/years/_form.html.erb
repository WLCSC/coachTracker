<%= semantic_form_for(@year) do |f| %>
<%= f.semantic_errors %>

<%= f.inputs do %>
  <%= f.input :id, label: 'Year (for the 2015-2016 school year, put 2015)', required: true %>
<% end %>

<%= f.actions do %>
  <%= f.action :submit, button_html: { disable_with: "Wait...", class: 'btn btn-primary' } %>
<% end %>
<% end %>
